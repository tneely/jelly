{
  "name": "cdk-jelly",
  "version": "0.7.0",
  "description": "A complete serverless web application infrastructure for the JAMstack",
  "main": "dist/jelly.js",
  "types": "dist/jelly.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tneely/jelly"
  },
  "author": {
    "name": "Taylor Neely",
    "url": "https://www.tneely.com"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "parcel build lambda/auth/index.ts --no-source-maps && tsc -b",
    "clean": "tsc -b --clean",
    "watch": "tsc -b -w & parcel watch lambda/auth/index.ts --no-source-maps",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint-fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "test": "jest --coverage",
    "prepare": "npm run build",
    "example-install": "cd example && npm ci && npm run bootstrap",
    "example-build": "cd example && npm run build",
    "example-synth": "cd example/packages/infrastructure && npm run synth"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.57.0",
    "@aws-cdk/aws-certificatemanager": "^1.57.0",
    "@aws-cdk/aws-cloudfront": "^1.57.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.57.0",
    "@aws-cdk/aws-codedeploy": "^1.57.0",
    "@aws-cdk/aws-cognito": "^1.57.0",
    "@aws-cdk/aws-dynamodb": "^1.57.0",
    "@aws-cdk/aws-iam": "^1.57.0",
    "@aws-cdk/aws-lambda": "^1.57.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.57.0",
    "@aws-cdk/aws-route53": "^1.57.0",
    "@aws-cdk/aws-route53-targets": "^1.57.0",
    "@aws-cdk/aws-s3": "^1.57.0",
    "@aws-cdk/aws-s3-deployment": "^1.57.0",
    "@aws-cdk/core": "^1.57.0",
    "axios": "^0.19.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "^1.57.0",
    "@aws-cdk/aws-certificatemanager": "^1.57.0",
    "@aws-cdk/aws-cloudfront": "^1.57.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.57.0",
    "@aws-cdk/aws-codedeploy": "^1.57.0",
    "@aws-cdk/aws-cognito": "^1.57.0",
    "@aws-cdk/aws-dynamodb": "^1.57.0",
    "@aws-cdk/aws-iam": "^1.57.0",
    "@aws-cdk/aws-lambda": "^1.57.0",
    "@aws-cdk/aws-route53": "^1.57.0",
    "@aws-cdk/aws-route53-targets": "^1.57.0",
    "@aws-cdk/aws-s3": "^1.57.0",
    "@aws-cdk/aws-s3-deployment": "^1.57.0",
    "@aws-cdk/core": "^1.57.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/node": "^12.12.54",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.9.0",
    "parcel": "^2.0.0-beta.1",
    "prettier": "^2.0.5",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "auth-lambda": "dist/lambda/authentication/index.js",
  "targets": {
    "main": false,
    "types": false,
    "auth-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "minify": true,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}
