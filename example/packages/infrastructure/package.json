{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "test": "jest",
    "preinstall": "npm run create-jelly-store && npm run move-jelly-dist && npm run move-jelly-pkg",
    "create-jelly-store": "mkdir -p .store/cdk-jelly",
    "move-jelly-dist": "cp -r ../../../dist .store/cdk-jelly",
    "move-jelly-pkg": "cp ../../../package.json .store/cdk-jelly/package.json",
    "synth": "cdk synth && mv cdk.out ../../../cdk.out"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.61",
    "@types/jest": "^25.2.1",
    "@types/node": "10.17.5",
    "aws-cdk": "^1.59.0",
    "jest": "^25.5.0",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.59.0",
    "@aws-cdk/aws-certificatemanager": "^1.59.0",
    "@aws-cdk/aws-cloudfront": "^1.59.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.59.0",
    "@aws-cdk/aws-codebuild": "^1.59.0",
    "@aws-cdk/aws-codedeploy": "^1.59.0",
    "@aws-cdk/aws-codepipeline": "^1.59.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.59.0",
    "@aws-cdk/aws-cognito": "^1.59.0",
    "@aws-cdk/aws-dynamodb": "^1.59.0",
    "@aws-cdk/aws-iam": "^1.59.0",
    "@aws-cdk/aws-lambda": "^1.59.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.59.0",
    "@aws-cdk/aws-route53": "^1.59.0",
    "@aws-cdk/aws-route53-targets": "^1.59.0",
    "@aws-cdk/aws-s3": "^1.59.0",
    "@aws-cdk/aws-s3-deployment": "^1.59.0",
    "@aws-cdk/core": "^1.59.0",
    "@aws-cdk/pipelines": "^1.59.0",
    "cdk-jelly": "file:.store/cdk-jelly"
  }
}
